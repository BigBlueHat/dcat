        "b": {"@id": "_:b", "@type": "xsd:integer" }
      },
      "distribution": {       
        "contentPath": "x1.csv"
      }
    },
    {
      "name": "csv2",
      "@context": {
        "xsd": "http://www.w3.org/2001/XMLSchema#",
